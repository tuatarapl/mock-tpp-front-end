{
  "name": "mock-tpp-front-end",
  "version": "1.0.0",
  "description": "Web UI of mock tpp used to test back end components",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha test/**/*.{ts,tsx}",
    "build": "rm -rf target && tsc",
    "format": "tslint 'src/**/*.ts' --fix",
    "lint": "tslint 'src/**/*.ts'",
    "start": "node target/server"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "author": "Grzegorz Abramczyk <grzegorz.abramczyk@tuatara.pl>",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express-session": "^1.15.10",
    "@types/passport": "^0.4.6",
    "@types/passport-local": "^1.0.33",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0",
    "source-map-support": "^0.5.8",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.16.0",
    "@types/session-file-store": "^1.2.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^5.0.0-alpha.7",
    "express-session": "^1.15.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "session-file-store": "^1.2.0"
  }
}
